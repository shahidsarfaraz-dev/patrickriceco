{
  "dependencies": {
    "sass": "^1.71.1"
  },
  "scripts": {
    "components": "sass --watch scss/components/_all_components.scss assets/css/all_components.css --style compressed",
    "base": "sass --watch scss/base/_base.scss assets/css/base.css --style compressed",
    "home": "sass --watch scss/pages/_home.scss assets/css/home.css --style compressed",
    "about": "sass --watch scss/pages/_about-us.scss assets/css/about-us.css --style compressed",
    "blog-archive": "sass --watch scss/pages/_blog-archive.scss assets/css/blog-archive.css --style compressed",
    "blog-single": "sass --watch scss/pages/_blog-single.scss assets/css/blog-single.css --style compressed",
    "get-free-strategy": "sass --watch scss/pages/_get-free-strategy.scss assets/css/get-free-strategy.css --style compressed",
    "contact-us": "sass --watch scss/pages/_contact-us.scss assets/css/contact-us.css --style compressed",
    "case-study": "sass --watch scss/pages/_case-study.scss assets/css/case-study.css --style compressed",
    "404-error": "sass --watch scss/pages/_404-error.scss assets/css/404-error.css --style compressed",
    "font": "sass --watch scss/base/_fonts.scss assets/css/fonts.css --style compressed",
    "compile-main-sass": "sass --watch theme_styles.scss theme_styles.css --style compressed",
    "compile-sass": "concurrently  \"npm run compile-main-sass\" \"npm run components\" \"npm run base\" \"npm run font\" \"npm run home\" \"npm run 404-error\" \"npm run blog-archive\" \"npm run blog-single\" \"npm run get-free-strategy\" \"npm run contact-us\" \"npm run case-study\""
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}